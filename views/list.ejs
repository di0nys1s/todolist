
    <%- include("header") -%>

        <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>
    <div class="box">
            <% newListItems.forEach(function(item) { %>
            <div class="item">
                <input type="checkbox">
                <p> <%= item.name %></p>
            </div>
            <% }) %>
        <form class="item" action="/" method="post">
            <input type="text" minlength="3" maxlength="50" name="newItem" placeholder="New Item" autocomplete="off" required>
            <button type="submit" name="list" value = <%= listTitle %>>+</button></button> </form>
    </div>

    <%- include("footer") -%>
